    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "xo": "^0.57.0"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "dotprop": "^1.2.1",
    "dset": "^3.1.3",
    "object-to-formdata": "^4.5.1",
    "qs": "^6.11.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
