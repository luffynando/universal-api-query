{
  "name": "universal-api-query",
  "version": "1.0.0",
  "description": "ðŸ’Ž Elegant and simple way to build requests for REST API",
  "keywords": [
    "api",
    "rest",
    "query",
    "builder",
    "laravel"
  ],
  "private": false,
  "author": {
    "name": "Fernando Isidro Luna",
    "email": "luffynando@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "is-ci || husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "rimraf dist && tsc",
    "release": "pnpm run build && pnpm changeset publish",
    "lint": "xo --fix",
    "lint:check": "xo",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#tests/*": "./tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luffynando/universal-api-query.git"
  },
  "bugs": {
    "url": "https://github.com/luffynando/universal-api-query/issues"
  },
  "homepage": "https://github.com/luffynando/universal-api-query#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/is-ci": "^3.0.4",
    "@types/qs": "^6.9.11",
    "@vitest/coverage-istanbul": "^1.2.2",
    "axios": "^1.6.7",
    "axios-mock-adapter": "^1.22.0",
    "husky": "^9.0.10",
    "is-ci": "^3.0.1",
